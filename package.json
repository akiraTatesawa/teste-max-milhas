{
  "name": "teste-max-milhas",
  "version": "1.0.0",
  "description": "API para cadastro e validação de CPFs",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "prepare": "ts-patch install -s",
    "start": "node -r tsconfig-paths/register dist/src/infra/http/server.js",
    "test": "jest",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --ignore-watch node_modules -r tsconfig-paths/register ./src/infra/http/server.ts",
    "dev:compose": "docker-compose -f docker-compose.development.yml --env-file .env.development",
    "dev:docker": "npm run dev:compose -- up",
    "dev:docker:build": "npm run dev:compose -- up --build",
    "dev:docker:down": "npm run dev:compose -- down --rmi all",
    "dev:docker:seed": "docker exec -it max-milhas-development npx prisma db seed",
    "ci:test": "jest --runInBand --verbose --coverage=true"
  },
  "prisma": {
    "schema": "./src/infra/data/prisma/schema.prisma",
    "seed": "ts-node ./src/infra/data/prisma/seeds/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akiraTatesawa/teste-max-milhas.git"
  },
  "author": "Arthur Akira Tatesawa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/akiraTatesawa/teste-max-milhas/issues"
  },
  "homepage": "https://github.com/akiraTatesawa/teste-max-milhas#readme",
  "dependencies": {
    "@ngneat/falso": "^6.4.0",
    "@prisma/client": "^4.10.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.6.2",
    "joi": "^17.8.3",
    "morgan": "^1.10.0",
    "node": "^19.1.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.18",
    "@swc/jest": "^0.2.23",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.2.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prisma": "^4.10.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^2.0.2",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "typescript-transform-paths": "^3.4.4"
  }
}
